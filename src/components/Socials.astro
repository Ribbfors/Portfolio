---
import { socials } from "@/data";
---

<ul class="flex flex-row gap-2">
  {
    socials.map((social, index) => (
      <>
        <li>
          <a href={social.link} target="_blank">{social.label}</a>
        </li>

        {socials.length - 1 !== index && <li>/</li>}
      </>
    ))
  }
</ul>
